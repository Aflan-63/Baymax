<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" prefHeight="346.0" prefWidth="605.0"
            xmlns="http://javafx.com/javafx/9" fx:controller="baymax2.LoginController">
    <children>
        <ImageView fitHeight="346.0" fitWidth="605.0" opacity="0.78" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@hosptal.jpg"/>
            </image>
        </ImageView>
        <Label fx:id="button1" layoutX="35.0" layoutY="73.0" text="Patient ID: " textFill="#1ec612">
            <font>
                <Font name="System Bold Italic" size="18.0"/>
            </font>
        </Label>
        <Label fx:id="button2" layoutX="38.0" layoutY="111.0" styleClass="park-label" text="Password: "
               textFill="#1aae29">
            <font>
                <Font name="System Bold Italic" size="18.0"/>
            </font>
        </Label>
        <Button fx:id="logInButton" layoutX="131.0" layoutY="157.0" mnemonicParsing="false"
                onAction="#logInButtonOnAction" styleClass="login-button" stylesheets="@StyleSheet.css" text="Log In"/>
        <Button fx:id="signUpButton" layoutX="206.0" layoutY="157.0" mnemonicParsing="false"
                onAction="#signUpButtonOnAction" styleClass="login-button" stylesheets="@StyleSheet.css"
                text="Sign Up"/>
        <TextField fx:id="t1" layoutX="132.0" layoutY="74.0"/>
        <TextField fx:id="t2" layoutX="132.0" layoutY="112.0"/>
        <Label fx:id="label" layoutX="367.0" layoutY="116.0" text="Label" textFill="#ae17a4" visible="false">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
