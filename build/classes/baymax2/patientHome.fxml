<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" prefHeight="338.0" prefWidth="604.0"
            xmlns="http://javafx.com/javafx/9" fx:controller="baymax2.PatientHomeController">
    <children>
        <ImageView fitHeight="400.0" fitWidth="600.0" layoutX="-2.0" layoutY="-9.0" opacity="0.44" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@hosptal.jpg"/>
            </image>
        </ImageView>
        <Button layoutX="290.0" layoutY="53.0" mnemonicParsing="false" onAction="#goButtonOnAction" prefHeight="25.0"
                prefWidth="150.0" styleClass="exit-button" stylesheets="@StyleSheet.css" text="GO"/>
        <ComboBox fx:id="comboDoc" layoutX="290.0" layoutY="139.0" prefWidth="150.0" promptText="Choose Doctor"/>
        <Label layoutX="14.0" layoutY="53.0" styleClass="park-label" stylesheets="@StyleSheet.css"
               text=" Show History: "/>
        <Label layoutX="26.0" layoutY="125.0" styleClass="park-label" stylesheets="@StyleSheet.css"
               text="Appoinment: "/>
        <Label fx:id="label" layoutX="479.0" layoutY="216.0" text="Label" visible="false"/>
        <Button layoutX="339.0" layoutY="191.0" mnemonicParsing="false" onAction="#meetButtonOnAction"
                style="-fx-background-color: #82da5c;" text="meet"/>
    </children>
</AnchorPane>
